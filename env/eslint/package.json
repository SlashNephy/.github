{
  "name": "@slashnephy/eslint-config",
  "version": "0.2.39",
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/eslint"
  },
  "license": "MIT",
  "main": "dist/src/index.js",
  "files": [
    "package.json",
    "dist/src/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "bump-version": "ts-node bin/bump-version.ts",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn build && yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "yarn build && eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "eslint-config-next": "13.1.3",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-css-import-order": "1.1.0",
    "eslint-plugin-editorconfig": "4.0.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-no-void-return-type": "1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-package-json": "0.1.4",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "eslint-plugin-userscripts": "0.2.10",
    "eslint-plugin-xss": "0.1.12",
    "eslint-plugin-yml": "1.4.0"
  },
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@slashnephy/prettier-config": "0.1.7",
    "@types/eslint": "8.4.10",
    "@types/node": "18.11.18",
    "@types/prettier": "2.7.2",
    "concurrently": "7.6.0",
    "eslint": "8.32.0",
    "prettier": "2.8.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.3.1",
  "eslintConfig": {
    "extends": [
      "./dist/src/index.js"
    ]
  }
}
