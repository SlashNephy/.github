{
  "name": "@slashnephy/eslint-config",
  "version": "1.0.7",
  "type": "commonjs",
  "main": "dist/index.js",
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/eslint"
  },
  "license": "MIT",
  "files": [
    "package.json",
    "dist/**/*.js",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "bump-version": "ts-node bin/bump-version.ts",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn build && yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "yarn build && eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "dependencies": {
    "@emotion/eslint-plugin": "11.11.0",
    "@next/eslint-plugin-next": "13.4.12",
    "@rushstack/eslint-patch": "1.3.3",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "8.9.0",
    "eslint-config-standard": "17.1.0",
    "eslint-gitignore": "0.1.0",
    "eslint-import-resolver-node": "0.3.7",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-css-import-order": "1.1.0",
    "eslint-plugin-css-reorder": "0.5.1",
    "eslint-plugin-deprecation": "1.5.0",
    "eslint-plugin-editorconfig": "4.0.3",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-n": "16.0.1",
    "eslint-plugin-no-void-return-type": "1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-package-json": "0.1.4",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-relay": "1.8.3",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-tsdoc": "0.2.17",
    "eslint-plugin-unused-imports": "3.0.0",
    "eslint-plugin-userscripts": "0.3.0",
    "eslint-plugin-vitest": "0.2.2",
    "eslint-plugin-xss": "0.1.12",
    "eslint-plugin-yml": "1.7.0",
    "typescript": "5.1.6",
    "yaml-eslint-parser": "1.2.2"
  },
  "peerDependencies": {
    "eslint": "^8"
  },
  "devDependencies": {
    "@slashnephy/prettier-config": "0.1.27",
    "@types/eslint": "8.44.1",
    "@types/node": "20.4.5",
    "concurrently": "8.2.0",
    "eslint": "8.45.0",
    "prettier": "3.0.0",
    "ts-node": "10.9.1"
  },
  "packageManager": "yarn@3.6.1",
  "eslintConfig": {
    "extends": "./dist"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ]
}
