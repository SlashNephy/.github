{
  "name": "@slashnephy/eslint-config",
  "version": "0.1.6",
  "repository": {
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/eslint"
  },
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "package.json",
    "dist/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "clean": "rm -rf dist/*",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-css-import-order": "1.1.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unused-imports": "2.0.0"
  },
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@slashnephy/prettier-config": "0.1.1",
    "@types/eslint": "8.4.3",
    "@types/node": "17.0.41",
    "@types/prettier": "2.6.3",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "./dist/index.js"
    ]
  }
}
