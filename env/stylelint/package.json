{
  "name": "@slashnephy/stylelint-config",
  "version": "0.1.1",
  "repository": {
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/stylelint"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "package.json",
    "dist/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "clean": "rm -rf dist/*",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "dependencies": {
    "stylelint-config-property-sort-order-smacss": "9.0.0",
    "stylelint-config-recommended": "8.0.0"
  },
  "peerDependencies": {
    "stylelint": "^14"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.1.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.41",
    "concurrently": "7.3.0",
    "eslint": "8.21.0",
    "prettier": "2.7.1",
    "stylelint": "14.9.1",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.2",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
