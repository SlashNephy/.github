{
  "name": "@slashnephy/stylelint-config",
  "version": "0.1.1",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/stylelint"
  },
  "license": "MIT",
  "files": [
    "package.json",
    "dist/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "dependencies": {
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-recommended": "10.0.1"
  },
  "peerDependencies": {
    "stylelint": "^14 || ^15.0.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.3.24",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/node": "18.15.5",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "prettier": "2.8.5",
    "stylelint": "15.3.0",
    "typescript": "5.0.2"
  },
  "packageManager": "yarn@3.4.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
