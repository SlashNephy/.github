{
  "name": "@slashnephy/stylelint-config",
  "version": "0.1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/stylelint"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "package.json",
    "dist/*.js",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "dependencies": {
    "stylelint-config-property-sort-order-smacss": "10.0.0",
    "stylelint-config-recommended": "16.0.0"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "2.1.3",
    "@slashnephy/prettier-config": "1.0.73",
    "@types/node": "20.4.5",
    "concurrently": "8.2.0",
    "eslint": "8.45.0",
    "prettier": "3.0.0",
    "stylelint": "15.10.2",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "stylelint": "^15"
  },
  "packageManager": "yarn@4.0.2",
  "publishConfig": {
    "access": "public"
  }
}
