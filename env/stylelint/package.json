{
  "name": "@slashnephy/stylelint-config",
  "version": "0.1.1",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/stylelint"
  },
  "license": "MIT",
  "files": [
    "package.json",
    "dist/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "dependencies": {
    "stylelint-config-property-sort-order-smacss": "9.1.0",
    "stylelint-config-recommended": "12.0.0"
  },
  "peerDependencies": {
    "stylelint": "^15"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.5.3",
    "@slashnephy/prettier-config": "0.1.24",
    "@types/node": "18.16.7",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "concurrently": "8.0.1",
    "eslint": "8.40.0",
    "eslint-plugin-deprecation": "1.4.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.8",
    "stylelint": "15.6.2",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
