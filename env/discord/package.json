{
  "name": "@slashnephy/discord",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/discord"
  },
  "scripts": {
    "build": "tsc",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "watch": "yarn bundle -w"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-typescript": "11.1.2",
    "@slashnephy/eslint-config": "0.5.3",
    "@slashnephy/prettier-config": "0.1.25",
    "@types/babel__core": "7.20.1",
    "@types/babel__preset-env": "7.9.2",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "concurrently": "8.0.1",
    "eslint": "8.40.0",
    "eslint-plugin-deprecation": "1.4.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.8",
    "rollup": "3.21.8",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "browserslist": [
    "Chrome 91.0.4472.164",
    "Electron 13.6.6"
  ]
}
