{
  "name": "@slashnephy/discord",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/discord"
  },
  "scripts": {
    "build": "tsc",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "watch": "yarn bundle -w"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.21.0",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-typescript": "11.0.0",
    "@slashnephy/eslint-config": "0.3.23",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/babel__core": "7.20.0",
    "@types/babel__preset-env": "7.9.2",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "prettier": "2.8.5",
    "rollup": "3.20.0",
    "typescript": "5.0.2"
  },
  "packageManager": "yarn@3.4.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "browserslist": [
    "Chrome 91.0.4472.164",
    "Electron 13.6.6"
  ]
}
