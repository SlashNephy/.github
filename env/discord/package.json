{
  "name": "@slashnephy/discord",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/discord"
  },
  "scripts": {
    "build": "tsc",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "watch": "yarn bundle -w"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/preset-typescript": "7.22.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-typescript": "11.1.2",
    "@slashnephy/eslint-config": "2.0.59",
    "@slashnephy/prettier-config": "1.0.41",
    "@types/babel__core": "7.20.1",
    "@types/babel__preset-env": "7.9.2",
    "concurrently": "8.2.0",
    "eslint": "8.45.0",
    "prettier": "3.0.0",
    "rollup": "3.26.3",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "browserslist": [
    "Chrome 91.0.4472.164",
    "Electron 13.6.6"
  ]
}
