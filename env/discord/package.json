{
  "name": "@slashnephy/discord",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/discord"
  },
  "scripts": {
    "build": "tsc",
    "bundle": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "watch": "yarn bundle -w"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "7.18.6",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-typescript": "9.0.2",
    "@slashnephy/eslint-config": "0.2.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/babel__core": "7.1.19",
    "@types/babel__preset-env": "7.9.2",
    "concurrently": "7.4.0",
    "eslint": "8.25.0",
    "prettier": "2.7.1",
    "rollup": "3.2.3",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.4",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "browserslist": [
    "Chrome 91.0.4472.164",
    "Electron 13.6.6"
  ]
}
