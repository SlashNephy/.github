{
  "name": "@slashnephy/typescript-extension",
  "version": "0.2.3",
  "main": "dist/index.js",
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/typescript"
  },
  "license": "MIT",
  "files": [
    "package.json",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "1.0.7",
    "@slashnephy/prettier-config": "0.1.27",
    "@types/node": "20.4.5",
    "concurrently": "8.2.0",
    "eslint": "8.45.0",
    "prettier": "3.0.3",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ],
    "overrides": [
      {
        "files": "src/**/*.ts",
        "rules": {
          "no-extend-native": "off",
          "func-names": "off"
        }
      }
    ]
  }
}
