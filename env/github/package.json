{
  "name": "@slashnephy/github-config",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/github"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "ts-node bin/update-webhooks.ts"
  },
  "dependencies": {
    "@octokit/rest": "19.0.7",
    "dotenv": "16.0.3",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.4.4",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/node": "18.15.7",
    "concurrently": "7.6.0",
    "eslint": "8.36.0",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "packageManager": "yarn@3.5.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
