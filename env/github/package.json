{
  "name": "@slashnephy/github-config",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/github"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node bin/main.ts",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write"
  },
  "dependencies": {
    "@octokit/rest": "19.0.3",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.1.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.41",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "prettier": "2.7.1",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
