{
  "name": "@slashnephy/github-config",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/github"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "ts-node bin/update-webhooks.ts"
  },
  "dependencies": {
    "@octokit/rest": "19.0.5",
    "dotenv": "16.0.3",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.2.25",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "18.11.13",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "prettier": "2.8.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@3.3.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
