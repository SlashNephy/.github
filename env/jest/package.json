{
  "name": "@slashnephy/jest-config",
  "version": "0.1.0",
  "repository": {
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/jest"
  },
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "package.json",
    "dist/**/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "clean": "rm -rf dist/*",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "peerDependencies": {
    "jest": "^28"
  },
  "devDependencies": {
    "@jest/types": "28.1.3",
    "@slashnephy/eslint-config": "0.1.7",
    "@slashnephy/prettier-config": "0.1.1",
    "@types/node": "17.0.41",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
