[include]
  path = ~/.github/env/git/.gitconfig

[gpg]
  format = ssh

[gpg "ssh"]
  # ~ が使えないらしい
  program = "C:/Users/spica/AppData/Local/1Password/app/8/op-ssh-sign.exe"

[core]
  sshCommand = C:/Windows/System32/OpenSSH/ssh.exe
  editor = \"~/AppData/Local/Programs/Microsoft VS Code/Code.exe\" --wait
  pager = LESSCHARSET=utf-8 less
  autocrlf = true
  hookspath =

[credential]
  helper = wincred

[http]
  postBuffer = 157286400
