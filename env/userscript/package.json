{
  "name": "@slashnephy/userscript",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/userscript"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "build:rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build:rollup:private": "rollup --config rollup.config.private.ts --configPlugin @rollup/plugin-typescript",
    "bundle": "yarn build:rollup",
    "bundle:private": "yarn build:rollup:private",
    "ci": "concurrently -n ci: -m 1 'yarn:ci:*'",
    "ci:bundle": "yarn bundle",
    "ci:generate-readme": "ts-node --project tsconfig.node.json bin/generate-readme.ts && yarn format:prettier",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "serve": "serve dist/",
    "watch": "yarn build:rollup -w",
    "watch:private": "yarn build:rollup:private -w"
  },
  "dependencies": {
    "@geolonia/japanese-numeral": "0.1.16",
    "@xpadev-net/niconicomments": "0.2.51",
    "fast-xml-parser": "4.2.4",
    "ts-array-length": "0.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "25.0.1",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-typescript": "11.1.1",
    "@slashnephy/eslint-config": "0.5.2",
    "@slashnephy/prettier-config": "0.1.24",
    "@types/babel__core": "7.20.1",
    "@types/babel__preset-env": "7.9.2",
    "@types/fuzzyset": "1.0.4",
    "@types/jquery": "3.5.16",
    "@types/node": "18.16.7",
    "@types/tampermonkey": "4.0.10",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "concurrently": "8.0.1",
    "eslint": "8.40.0",
    "eslint-plugin-deprecation": "1.4.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "markdown-doc-builder": "1.3.0",
    "prettier": "2.8.8",
    "rollup": "3.21.8",
    "serve": "14.2.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "packageManager": "yarn@3.5.1",
  "engines": {
    "node": ">= 18"
  },
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  },
  "browserslist": [
    "> 0.25%",
    "not dead"
  ],
  "userscriptVersions": {
    "tampermonkey": ">= 4"
  }
}
