{
  "name": "@slashnephy/prettier-config",
  "version": "0.1.16",
  "author": "SlashNephy <spica@starry.blue> (https://spica.starry.blue/)",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/.github",
    "directory": "env/prettier"
  },
  "license": "MIT",
  "main": "dist/src/index.js",
  "files": [
    "package.json",
    "dist/src/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "bump-version": "ts-node bin/bump-version.ts",
    "clean": "rm -rf dist/",
    "format": "concurrently -n format: 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn clean && yarn build && yarn npm publish"
  },
  "peerDependencies": {
    "prettier": "^2"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.4.22",
    "@types/node": "18.16.6",
    "@types/prettier": "2.7.2",
    "concurrently": "8.0.1",
    "eslint": "8.40.0",
    "prettier": "2.8.8",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "packageManager": "yarn@3.5.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}