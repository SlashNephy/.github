{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  description: 'Grouping',
  extends: ['group:recommended'],
  packageRules: [
    {
      groupName: 'Mantine',
      matchPackagePrefixes: ['@mantine/'],
    },
    {
      groupName: 'Ktor',
      matchPackagePrefixes: ['io.ktor:'],
    },
    {
      groupName: 'Kotlin',
      matchPackagePrefixes: ['org.jetbrains.kotlin.'],
    },
  ],
}
