{
  "name": "@slashnephy/types-amq",
  "version": "0.1.0",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/types-amq"
  },
  "files": [
    "package.json",
    "types/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bump-version": "ts-node bin/bump-version.ts",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn npm publish"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "0.4.16",
    "@slashnephy/prettier-config": "0.1.16",
    "@types/jquery": "3.5.16",
    "@types/node": "18.16.0",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "concurrently": "8.0.1",
    "dts-gen": "0.6.1",
    "eslint": "8.39.0",
    "eslint-plugin-deprecation": "1.4.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-unused-imports": "2.0.0",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.2"
  },
  "packageManager": "yarn@3.5.0",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
