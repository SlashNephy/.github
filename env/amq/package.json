{
  "name": "@slashnephy/types-amq",
  "version": "0.1.8",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/SlashNephy/types-amq"
  },
  "files": [
    "package.json",
    "types/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --noEmit",
    "bump-version": "ts-node bin/bump-version.ts",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "generate": "tsc src/**/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "publish": "yarn npm publish"
  },
  "dependencies": {
    "@types/jquery": "3.5.16"
  },
  "devDependencies": {
    "@slashnephy/eslint-config": "1.0.6",
    "@slashnephy/prettier-config": "0.1.27",
    "@types/node": "20.4.5",
    "concurrently": "8.2.0",
    "dts-gen": "0.6.1",
    "eslint": "8.45.0",
    "prettier": "3.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@3.6.1",
  "eslintConfig": {
    "extends": [
      "@slashnephy/eslint-config"
    ]
  }
}
